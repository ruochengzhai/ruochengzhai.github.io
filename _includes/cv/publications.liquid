<ol class="list-group list-group-numbered">
  {% for pub in entry.contents %}
    <li class="list-group-item">
      {{ pub.authors | markdownify | strip}}, {{ pub.year }}, {{ pub.journal }},
      <a href="https://doi.org/{{ pub.doi }}">{{ pub.doi }}</a><br>
      <em>{{ pub.title | markdownify | strip}}</em>
    </li>
  {% endfor %}
</ol>